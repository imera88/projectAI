  data['audio'].append(audio_file)\n","                data['text'].append(transcription)\n","            else:\n","                print(f\"Archivo de audio no encontrado: {audio_file}\")\n","\n","    # Convertir a Dataset de Hugging Face\n","    dataset = Dataset.from_dict(data).cast_column(\"audio\", Audio(sampling_rate=16_000))\n","    return dataset"]},{"cell_type":"code","source":["# Crear el dataset\n","dataset = create_dataset_from_multiple_csvs(base_path)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"EP6LlZ1N7klQ","executionInfo":{"status":"ok","timestamp":1734248262464,"user_tz":300,"elapsed":51142,"user":{"displayName":"Marcos Irving Mera Sanchez","userId":"00640408618716900836"}},"outputId":"35e4b212-4c8f-4dc2-8432-ef88b8e059a1"},"execution_count":4,"outputs":[{"output_type":"stream","name":"stdout","text":["Se encontraron 327 archivos CSV.\n"]}]},{"cell_type":"code","execution_count":5,"metadata":{"executionInfo":{"elapsed":78,"status":"ok","timestamp":1734248262468,"user":{"displayName":"Marcos Irving Mera Sanchez","userId":"00640408618716900836"},"user_tz":300},"id":"CAWAdzoFPts2"},"outputs":[],"source":["# Preprocess transcription function\n","def preprocess_transcription(transcription):\n","    return transcription.lower().strip()\n","\n","# Define training function\n","def train_model(model, dataloader, device):\n","    model.to(device)\n","    model.eval()\n","    predictions = []\n","    references = []\n","\n","    with torch.no_grad():\n","        for audio, transcript in dataloader:\n","            audio = audio.to(device)\n","            result = model.transcribe(audio)\n","            pred_text = result[\"text\"]\n","            ref_text = preprocess_transcription(transcript)\n","\n","            predictions.append(pred_text)\n","            references.append(ref_text)\n","\n","    return predictions, references\n","\n","# Calculate evaluation metrics\n","def calculate_metrics(predictions, references):\n","    metrics = {\n","        \"WER\": wer(references, predictions),\n","        \"CER\": cer(references, predictions)\n","    }\n","\n","    # F1, Precision, and Recall at the word level\n","    preds_split = [p.split() for p in predictions]\n","    refs_split = [r.split() for r in references]\n","\n","    all_preds = sum(preds_split, [])\n","    all_refs = sum(refs_split, [])\n","\n","    metrics[\"Precision\"] = precision_score(all_refs, all_preds, average=\"weighted\")\n","    metrics[\"Recall\"] = recall_score(all_refs, all_preds, average=\"weighted\")\n","    metrics[\"F1\"] = f1_score(all_refs, all_preds, average=\"weighted\")\n","\n","    return metrics\n","\n","def transcribe_audio(example):\n","    # Transferir datos de audio al dispositivo\n","    audio_data = torch.tensor(example[\"audio\"][\"array\"], dtype=torch.float32).to(device)\n","\n","    # Asegurarse de que el modelo también esté en el dispositivo\n","    inputs = processor(audio_data, sampling_rate=16000, return_tensors=\"pt\", padding=True).input_values.to(device)\n","\n","    # Pasar los datos al modelo\n","    logits = model(inputs).logits\n","\n","    # Decodificar las predicciones\n","    predicted_ids = torch.argmax(logits, dim=-1)\n","    transcription = processor.batch_decode(predicted_ids)[0]\n","    return {\"predicted_text\": transcription}\n","\n","def normalize_audio(audio_array):\n","    \"\"\"\n","    Normaliza el audio para que tenga valores entre -1 y 1.\n","    \"\"\"\n","    max_val = np.max(np.abs(audio_array))\n","    return audio_array / max_val if max_val > 0 else audio_array\n","\n","def remove_noise(audio_array, sample_rate):\n","    \"\"\"\n","    Aplica un filtro pasa-bajas para reducir el ruido.\n","    \"\"\"\n","    lowpass_filter = torchaudio.functional.lowpass_biquad(\n","        torch.tensor(audio_array), sample_rate, cutoff_freq=3000\n","    )\n","    return lowpass_filter.numpy()\n","\n","#def transcribe_audio(example):\n","#    # Normalización del audio\n","#    audio_data = normalize_audio(example[\"audio\"][\"array\"])\n","\n","#    # Eliminación de ruido\n","#    sample_rate = example[\"audio\"][\"sampling_rate\"]\n","#    audio_data = remove_noise(audio_data, sample_rate)\n","\n","    # Transferir datos de audio al dispositivo\n","#    audio_data = torch.tensor(audio_data, dtype=torch.float32).to(device)\n","\n","    # Procesar los datos de entrada\n","#    inputs = processor(audio_data, sampling_rate=16000, return_tensors=\"pt\", padding=True).input_values.to(device)\n","\n","    # Pasar los datos al modelo\n","#    with torch.no_grad():\n","#        logits = model(inputs).logits\n","\n","    # Decodificar las predicciones usando estrategias avanzadas\n","#    predicted_ids = torch.argmax(logits, dim=-1)\n","#    transcription = processor.batch_decode(predicted_ids, skip_special_tokens=True)[0]\n","\n","    # Post-procesamiento de texto\n","#   transcription = post_process_transcription(transcription)\n","\n","#    return {\"predicted_text\": transcription}\n","\n","def post_process_transcription(text):\n","    \"\"\"\n","    Aplica técnicas de post-procesamiento al texto transcrito.\n","    \"\"\"\n","    import re\n","    text = text.lower()  # Convertir a minúsculas\n","    text = re.sub(r'[^\\w\\s]', '', text)  # Eliminar caracteres no alfanuméricos\n","    text = re.sub(r'\\s+', ' ', text).strip()  # Eliminar espacios múltiples y ajustar bordes\n","    return text\n","\n","\n","# Evaluar métricas WER y CER\n","def evaluate_metrics(reference, hypothesis):\n","    wer = jiwer.wer(reference, hypothesis)\n","    cer = jiwer.cer(reference, hypothesis)\n","    mer = jiwer.mer(reference, hypothesis)\n","    return wer, cer, mer\n","\n","def evaluate_performance(dataset):\n","    wer_scores, cer_scores, mer_scores = [], [], []\n","    for ref, pred in zip(dataset[\"text\"], dataset[\"predicted_text\"]):\n","        wer_scores.append(wer(ref, pred))\n","        cer_scores.append(cer(ref, pred))\n","        mer_scores.append(mer(ref, pred))\n","        print(\"ref\",ref)\n","        print(\"pred\",pred)\n","        print(\"wer\",wer(ref, pred))\n","        print(\"cer\",cer(ref, pred))\n","    return np.mean(wer_scores), np.mean(cer_scores)"]},{"cell_type":"code","source":["import torch\n","\n","# Cargar el modelo y procesador para español\n","model_name = \"jonatasgrosman/wav2vec2-large-xlsr-53-spanish\"\n","#model_name = \"facebook/wav2vec2-large-xlsr-53\"\n","processor = Wav2Vec2Processor.from_pretrained(model_name)\n","model = Wav2Vec2ForCTC.from_pretrained(model_name)\n","\n","\n","device = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\n","print(f\"Usando dispositivo: {device}\")\n","\n","model = model.to(device)\n","\n","dataset1 = dataset.map(transcribe_audio, remove_columns=[\"audio\"], load_from_cache_file=False)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":385,"referenced_widgets":["eeebb81d97754c719dfe688dffea90de","6bdeacdbe28e452fb6d4480ae0f1f2d0","5ff3f1f58efd4d96b290916fb018fbba","c3b03735edf047d4ac10b6ad21d6c0a1","5329ec0936194936a3735ae7a9195674","1ec38e0b3d5a44dea988711033da99d3","3243ddc943bb402cabfdf3a2cb26c6d2","236c5f6f2ed940419b023b520ab2cc34","e091763202ee438ba829c7624f9ebc50","f0779607a1364f99965d773f9fb97955","7da841df94054c9788fc30c5d0849cde","79bf7208bb014e7a89ca0dba33f52d86","9462566041bc49d9a686ccb37f3ecfb9","31b93623447b494a89ab219ca2a94d7c","eb5b6a2e799e4ba8a3ad7bd8a2ab3e92","6eab66cd4124463fb10697ef7ac2f124","3841b4ecae7f41388679efab83aecfda","d922329a14cc45c4ac1988e3e5828ac8","7fcf1caea1144267a7c9ba70cb510ea5","1b8d7a3beaac47a5a36977f20707bcca","4f01f46c08f54f6abf56dab8014bbd4f","ef9234490e834624b7a523b9989dc3bc","21a7d3611b3b47d78dca181211d89434","9a90b0c91ddb42c190097df57601ebad","c5a22d442328480b81ddb6711413e83e","fdd280ccca214b3b8c5f7dd0a9cd5fa4","856f592bab8c4b269bd23e86fea4fca9","b906fd4377984e9b9ce3025b2e77ed17","b9fc7e5331994423a4887b9c5aed6a10","930a7437e7cc480abc16346a19724d24","1e9800e8acd846caa83068486af60b72